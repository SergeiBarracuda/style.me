{
  "version": 2,
  "name": "beauty-marketplace-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_AUTH_COOKIE_NAME": "@next_public_auth_cookie_name",
    "NEXT_PUBLIC_AUTH_TOKEN_EXPIRY": "@next_public_auth_token_expiry"
  }
}
